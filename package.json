{
  "name": "laji-map",
  "version": "1.0.383",
  "description": "A map with drawing functionality",
  "main": "lib/map.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline --port 4000 --host 0.0.0.0",
    "build:lib": "rimraf lib && NODE_ENV=production tsc && copyfiles src/styles.css lib/styles.css",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "build": "npm run build:lib && npm run build:dist",
    "publish-to-npm": "npm run lint && npm version patch && git push && npm run build && npm publish",
    "push-to-git": "npm run lint && git push",
    "lint": "eslint -c .eslintrc.prod.json --ignore-pattern src/lib/ src playground"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/luomus/laji-map.js"
  },
  "keywords": [
    "leaflet",
    "leaflet.draw",
    "laji-map",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "license": "MIT",
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.0",
    "copyfiles": "^2.0.0",
    "css-loader": "^1.0.0",
    "eslint": "^3.19.0",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.1",
    "rimraf": "2.5.4",
    "style-loader": "^0.18.2",
    "svg-url-loader": "^2.3.2",
    "typescript": "^2.9.2",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@types/leaflet": "^1.2.8",
    "@types/leaflet-draw": "^0.4.13",
    "@types/leaflet-markercluster": "^1.0.3",
    "@types/leaflet.markercluster": "^1.0.3",
    "@types/proj4leaflet": "^1.0.5",
    "Leaflet.vector-markers": "git+https://git@github.com/olzraiti/Leaflet.vector-markers.git#17601bee91f2dae098a0a76e96714d59ec958fe6",
    "leaflet": "1.3.1",
    "leaflet-contextmenu": "1.4.0",
    "leaflet-draw": "1.0.0",
    "leaflet-geometryutil": "0.8.0",
    "leaflet-mml-layers": "2.0.1",
    "leaflet-textpath": "git+https://git@github.com/olzraiti/Leaflet.textPath.git#7fd168f22e2b1a52bdf62d473237f01caaa01e95",
    "leaflet.markercluster": "1.3.0",
    "nouislider": "10.0.0",
    "proj4": "2.3.16",
    "proj4leaflet": "1.0.2"
  }
}
