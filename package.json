{
  "name": "laji-map",
  "version": "3.2.2",
  "description": "A map with drawing functionality",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline --port 4000 --host 0.0.0.0",
    "build:lib": "rimraf lib && NODE_ENV=production tsc -p tsconfig.lib.json --declaration && copyfiles -f src/styles.css lib/",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "build": "npm run build:lib && npm run build:dist",
    "preversion": "npm run lint && npm run test:slow && npm run build",
    "version": "./update-changelog.sh",
    "postversion": "npm publish && git push",
    "lint": "tslint src/*.ts",
    "test": "HOST=127.0.0.1 PORT=4000 TEST_BROWSER=chrome protractor protractor.conf.js",
    "test:debug": "HOST=127.0.0.1 PORT=4000 TEST_BROWSER=chrome HEADLESS=false node --inspect-brk node_modules/protractor/bin/protractor protractor.conf.js",
    "test:slow": "HOST=127.0.0.1 PORT=4000 TEST_BROWSER=chrome DELAY=500 protractor protractor.conf.js"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/luomus/laji-map.js"
  },
  "keywords": [
    "leaflet",
    "leaflet.draw",
    "laji-map",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "license": "MIT",
  "devDependencies": {
    "@types/geojson": "*",
    "@types/leaflet": "*",
    "@types/leaflet-draw": "*",
    "@types/leaflet.markercluster": "*",
    "@types/proj4leaflet": "*",
    "awesome-typescript-loader": "^5.2.0",
    "copyfiles": "^2.0.0",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.11",
    "jasmine-spec-reporter": "^4.2.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^0.4.1",
    "protractor": "^6.0.0",
    "protractor-console-plugin": "^0.1.1",
    "rimraf": "2.5.4",
    "style-loader": "^0.18.2",
    "svg-url-loader": "^2.3.2",
    "ts-lint": "^4.5.1",
    "typescript": "^3.9.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "Leaflet.vector-markers": "git+https://git@github.com/olzraiti/Leaflet.vector-markers.git#17601bee91f2dae098a0a76e96714d59ec958fe6",
    "leaflet": "1.6.0",
    "leaflet-contextmenu": "1.4.0",
    "leaflet-draw": "1.0.3",
    "leaflet-geometryutil": "0.8.0",
    "leaflet-geosearch": "git+https://git@github.com/olzraiti/leaflet-geosearch.git#d0fb4384742e22799074355a99094adcc2416577",
    "leaflet-textpath": "git+https://git@github.com/olzraiti/Leaflet.textPath.git#7fd168f22e2b1a52bdf62d473237f01caaa01e95",
    "leaflet.markercluster": "1.3.0",
    "leaflet.smoothwheelzoom": "git+https://git@github.com/olzraiti/Leaflet.SmoothWheelZoom.git#18344d3e9179128085bcccfecd184a03b61a3d0c",
    "nouislider": "13.1.0",
    "proj4": "2.3.16",
    "proj4leaflet": "1.0.2"
  },
  "optionalDependencies": {
    "@types/geojson": "*",
    "@types/leaflet": "*",
    "@types/leaflet-draw": "*",
    "@types/leaflet.markercluster": "*",
    "@types/node": "^8.10.52",
    "@types/proj4leaflet": "*"
  },
  "resolutions": {
    "leaflet": "1.6.0",
    "proj4": "2.3.16"
  }
}
