  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "*",
    "css-loader": "*",
    "eslint": "*",
    "eslint-plugin-react": "*",
    "file-loader": "*",
    "react-hot-loader": "*",
    "style-loader": "*",
    "svg-url-loader": "^1.1.0",
    "url-loader": "*",
    "webpack": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "*",
    "babel-preset-react": "*",
    "babel-preset-stage-0": "*",
    "deeper": "*",
