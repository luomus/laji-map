{
  "name": "laji-map",
  "version": "1.0.122",
  "description": "A map with drawing functionality",
  "main": "dist/laji-map.js",
  "types": "src/map.d.js",
  "scripts": {
    "start": "webpack-dev-server --content-base playground playground/app.js --inline --port 4000",
    "build:lib": "rimraf lib && NODE_ENV=production babel -d lib/ src/ --copy-files",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "build": "npm run build:lib && npm run build:dist",
    "publish-to-npm": "npm version patch && git push && npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/luomus/laji-map.js"
  },
  "keywords": [
    "leaflet",
    "leaflet.draw",
    "laji-map",
    "luomus"
  ],
  "author": "Kalle Myllymaa <kalle.myllymaa@helsinki.fi>, Olli Raitio <olli.raitio@helsinki.fi>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "6.16.0",
    "babel-core": "6.17.0",
    "babel-loader": "6.2.5",
    "babel-plugin-array-includes": "2.0.3",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.16.0",
    "babel-preset-stage-0": "6.16.0",
    "css-loader": "0.25.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "json-loader": "0.5.4",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "svg-url-loader": "1.1.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "Leaflet.vector-markers": "git+https://git@github.com/olzraiti/Leaflet.vector-markers.git#17601bee91f2dae098a0a76e96714d59ec958fe6",
    "babel-runtime": "^6.18.0",
    "detect-browser": "^1.5.0",
    "leaflet": "^1.0.1",
    "leaflet-contextmenu": "^1.1.1",
    "leaflet-draw": "^0.4.1",
    "leaflet-mml-layers": "^2.0.1",
    "leaflet.markercluster": "^1.0.0",
    "proj4": "^2.3.15",
    "proj4leaflet": "git+https://git@github.com/kartena/Proj4Leaflet.git#3019d278b5d3e42a72b65142db2d5f0b06039b53"
  }
}
