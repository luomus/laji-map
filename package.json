{
  "name": "laji-map",
  "version": "3.14.4",
  "description": "A map with drawing functionality",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepare": "self-import",
    "start": "npx webpack serve",
    "build:lib": "rimraf lib && NODE_ENV=production tsc -p tsconfig.lib.json && copyfiles -f src/styles.css lib/",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "build:tests": "rimraf test-export && NODE_ENV=production tsc -p tsconfig.test.json",
    "build": "npm run build:lib && npm run build:dist && npm run build:tests",
    "preversion": "npm run lint && npm run test:multi:slow && npm run build",
    "version": "./update-changelog.sh",
    "postversion": "npm publish && git push",
    "lint": "eslint -c .eslintrc.prod.json src playground",
    "test": "HOST=127.0.0.1 PORT=4000 protractor protractor.conf.cjs",
    "test:debug": "HEADLESS=false HOST=127.0.0.1 PORT=4000 node --inspect-brk node_modules/protractor/bin/protractor protractor.conf.js",
    "test:slow": "DELAY=500 HOST=127.0.0.1 PORT=4000 protractor protractor.conf.cjs",
    "test:multi": "TEST_BROWSER=multi HOST=127.0.0.1 PORT=4000 protractor protractor.conf.cjs",
    "test:multi:slow": "TEST_BROWSER=multi DELAY=500 HOST=127.0.0.1 PORT=4000 protractor protractor.conf.cjs",
    "test:ffox": "TEST_BROWSER=firefox HOST=127.0.0.1 PORT=4000 protractor protractor.conf.cjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luomus/laji-map.git"
  },
  "keywords": [
    "leaflet",
    "leaflet.draw",
    "laji-map",
    "luomus"
  ],
  "author": "Olli Raitio <olli.raitio@helsinki.fi>",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "^3.6.1",
    "@types/jasminewd2": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.6",
    "eslint": "^7.29.0",
    "eslint-plugin-protractor": "^2.1.1",
    "jasmine-spec-reporter": "^4.2.1",
    "json-loader": "^0.5.7",
    "mini-css-extract-plugin": "^1.6.2",
    "protractor": "^7.0.0",
    "protractor-console-plugin": "^0.1.1",
    "rimraf": "2.5.4",
    "self-import": "^1.0.1",
    "style-loader": "^3.0.0",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^9.0.0",
    "typescript": "^4.3.4",
    "webpack": "^5.41.1",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  },
  "dependencies": {
    "@types/geojson": "*",
    "@types/leaflet": "*",
    "@types/leaflet-draw": "*",
    "@types/leaflet.markercluster": "*",
    "@types/node": "^8.10.52",
    "@types/proj4leaflet": "*",
    "Leaflet.vector-markers": "git+https://git@github.com/olzraiti/Leaflet.vector-markers.git#17601bee91f2dae098a0a76e96714d59ec958fe6",
    "leaflet": "1.7.1",
    "leaflet-contextmenu": "1.4.0",
    "leaflet-draw": "1.0.3",
    "leaflet-geometryutil": "0.8.0",
    "leaflet-geosearch": "^3.6.0",
    "leaflet-measure-path": "git+https://git@github.com/olzraiti/leaflet-measure-path.git#469f49ac48703e5ae22e972593946a279a3dac8e",
    "leaflet-textpath": "git+https://git@github.com/olzraiti/Leaflet.textPath.git#61b77ff750be090f748208532aa5a5137862b541",
    "leaflet.markercluster": "1.3.0",
    "leaflet.nontiledlayer": "^1.0.9",
    "leaflet.smoothwheelzoom": "git+https://git@github.com/olzraiti/Leaflet.SmoothWheelZoom.git#688a55b325cfcad618952c036636743811d9d01d",
    "nouislider": "13.1.0",
    "proj4": "2.3.16",
    "proj4leaflet": "1.0.2"
  },
  "resolutions": {
    "leaflet": "1.7.0",
    "proj4": "2.3.16",
    "leaflet-textpath": "git+https://git@github.com/olzraiti/Leaflet.textPath.git#61b77ff750be090f748208532aa5a5137862b541"
  }
}
