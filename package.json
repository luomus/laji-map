{
  "name": "laji-map",
  "version": "1.0.1",
  "description": "A map with drawing functionality",
  "main": "dist/laji-map.js",
  "scripts": {
	  "start": "webpack-dev-server --content-base playground playground/app.js --inline --port 4000",
	  "lint": "eslint src playground --ignore-pattern src/lib/*",
    "build:lib": "rimraf lib && NODE_ENV=production babel -d lib/ src/ --copy-files",
    "build:dist": "rimraf dist && NODE_ENV=production webpack --config webpack.config.dist.js && rimraf dist/styles.js",
    "publish-to-npm": "npm run build:dist && npm run build:lib"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/luomus/laji-map.js"
  },
  "keywords": [
    "leaflet",
    "leaflet.draw",
    "laji-map",
    "luomus"
  ],
  "author": "Kalle Myllymaa <kalle.myllymaa@helsinki.fi>, Olli Raitio <olli.raitio@helsinki.fi>",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "*",
    "eslint": "*",
    "webpack-dev-server": "*"
  },
  "dependencies": {
    "Leaflet.vector-markers": "git+https://git@github.com/hiasinho/Leaflet.vector-markers.git",
    "babel-cli": "*",
    "babel-core": "*",
    "babel-loader": "*",
    "babel-preset-es2015": "*",
    "babel-preset-stage-0": "*",
    "extract-text-webpack-plugin": "^1.0.1",
    "json-loader": "^0.5.4",
    "leaflet": "*",
    "leaflet-contextmenu": "git+https://git@github.com/aratcliffe/Leaflet.contextmenu.git",
    "leaflet-draw": "*",
    "proj4leaflet": "*",
    "rimraf": "*",
	  "file-loader": "*",
	  "style-loader": "*",
	  "svg-url-loader": "*",
	  "webpack": "*",
	  "url-loader": "*",
	  "css-loader": "*"
  }
}
